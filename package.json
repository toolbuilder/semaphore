{
  "name": "@toolbuilder/semaphore",
  "description": "Basic semaphore and mutex with both sync and async acquire methods.",
  "version": "0.1.0",
  "keywords": [
    "semaphore",
    "mutex",
    "acquire",
    "async"
  ],
  "homepage": "https://github.com/toolbuilder/semaphore#readme",
  "bugs": {
    "url": "https://github.com/toolbuilder/semaphore/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/toolbuilder/semaphore.git"
  },
  "license": "MIT",
  "main": "index.js",
  "module": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "lint:packagejson": "npmPkgJsonLint .",
    "lint:src": "eslint src test",
    "release": "run-s uncommitted validate standard-version",
    "standard-version": "standard-version",
    "test": "tape -r esm \"test/*.test.js\"",
    "test:packfile": "rollup --config node:@toolbuilder/rollup-config-pkgtest",
    "uncommitted": "uncommitted",
    "validate": "run-s test:* lint:*"
  },
  "eslintConfig": {
    "extends": "@toolbuilder/eslint-config"
  },
  "dependencies": {
    "esm": "^3.2.25"
  },
  "devDependencies": {
    "@toolbuilder/eslint-config": "0.1.3",
    "@toolbuilder/package-json-lint-config": "0.1.0",
    "@toolbuilder/rollup-config-pkgtest": "0.1.0",
    "eslint": "7.3.1",
    "npm-package-json-lint": "5.1.0",
    "npm-run-all": "4.1.5",
    "rollup": "2.18.0",
    "standard-version": "8.0.0",
    "tape": "^5.0.0",
    "uncommitted": "1.0.0",
    "zora": "^3.1.8"
  },
  "npmpackagejsonlint": {
    "extends": "@toolbuilder/package-json-lint-config"
  }
}
